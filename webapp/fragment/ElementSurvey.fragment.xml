<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog showHeader="true" stretch="{device>/system/phone}" id="idElementSurveyFragment">
		<content></content>
		<buttons>
			<Button text="{i18n>surveyIsFilled}" icon="sap-icon://survey" type="Accept" press="onSurveyFinished"/>
			<Button text="{i18n>labelCancel}" icon="sap-icon://decline" type="Reject" press="onSurveyClose"
				visible="{formDetailsModel>/surveyCloseButtonVisible}"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>